# Gemini AI Configuration (if needed)
# GOOGLE_API_KEY=your_gemini_api_key_here

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=false

# Security (if needed)
# SECRET_KEY=your_secret_key_here
# CORS_ORIGINS=http://localhost:3000,https://your-frontend-domain.com 