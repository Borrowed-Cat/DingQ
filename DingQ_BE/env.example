# Database Configuration
DATABASE_URL=postgresql://dingq_user:dingq_password@postgres:5432/dingq_db

# PostgreSQL Settings
POSTGRES_USER=dingq_user
POSTGRES_PASSWORD=dingq_password
POSTGRES_DB=dingq_db

# FastAPI Settings
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000

# Model Settings
CLIP_MODEL_NAME=openai/clip-vit-base-patch32
VECTOR_DATA_PATH=model/vectorweight.npz

# Logging
LOG_LEVEL=INFO

# OpenAI API Configuration (for Deep Search)
OPENAI_API_KEY=your_openai_api_key_here

# Cloud Storage Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json
SVG_BUCKET_NAME=dingq-svg-icons

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# 모델 설정
MODEL_DEVICE=cpu
FEATURE_DIM=512

# 벡터 스토어 설정
VECTOR_STORE_PATH=./vector_store 